{
  "name": "Section",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "section": {
      "type": "number",
      "required": true
    },
    "capacity": {
      "type": "number"
    },
    "course_id": {
      "type": "number"
    },
    "nama_pensyarah": {
      "type": "string"
    },
    "dateCreated": {
      "type": "Date",
      "defaultFn": "now"
    }

  },
  "validations": [],
  "relations": {
      "table": {
        "type": "hasMany",
        "model": "Table",
        "foreignKey": "section_id"
      },
      "course": {
        "type": "belongsTo",
        "model": "Course",
        "foreignKey": "course_id"
      },
      "registration": {
        "type": "hasMany",
        "model": "Registration",
        "foreignKey": "section_id"
      }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    },
    {
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW",
      "property": [
        "find",
        "count",
        "register",
        "listSection",
        "updateInfo",
        "updateSection"
      ]
    },
    {
      "principalType": "ROLE",
      "principalId": "manager",
      "permission": "ALLOW",
      "property": [
        "find",
        "count"
      ]
    }
  ],
  "methods": {}
}
