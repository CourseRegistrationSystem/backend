{
  "name": "Student",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "matricNo": {
      "type": "string",
      "required": true
    },
    "sesi": {
      "type": "string",
      "required": true
    },
    "semester": {
      "type": "number",
      "required": true
    },
    "createdDate": {
      "type": "Date",
      "defaultFn": "now"
    },
    "modifiedDate": {
      "type": "date"
    }

  },
  "validations": [],
  "relations": {
    "registration": {
      "type": "hasMany",
      "model": "Registration",
      "foreignKey": "student_id"
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    },
    {
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW",
      "property": [
        "find",
        "count",
        "register",
        "listStudent",
        "updateInfo",
        "updateStudent"
      ]
    },
    {
      "principalType": "ROLE",
      "principalId": "manager",
      "permission": "ALLOW",
      "property": [
        "find",
        "count"
      ]
    }
  ],
  "methods": {}
}
